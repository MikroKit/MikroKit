{
  "name": "@mikrokit/codegen",
  "version": "0.2.1-alpha.0",
  "description": "Browser client for Mikrokit Apps.",
  "keywords": [
    "typescript",
    "API",
    "RPC",
    "json",
    "schema",
    "generate",
    "server",
    "serverless",
    "framework",
    "node"
  ],
  "author": "ma jerez <jerez.m82@gmail.com>",
  "homepage": "https://github.com/mikrokit/mikrokit/tree/main/packages/client#readme",
  "license": "MIT",
  "main": "./.dist/cjs/index.js",
  "module": "./.dist/esm/index.js",
  "types": "./.dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./.dist/cjs/index.js",
      "default": "./.dist/esm/index.js"
    }
  },
  "directories": {
    "lib": ".dist"
  },
  "files": [
    ".dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikrokit/mikrokit.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "dev": "rimraf .dist && tsc --build tsconfig.build.json --watch",
    "dev:test": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "build:csj": "rimraf .dist/csj && tsc --build tsconfig.build.json",
    "build:esm": "rimraf .dist/esm && tsc --build tsconfig.build-esm.json",
    "build": "npm run build:csj && npm run build:esm",
    "auto-readme": "embedme **/*.md && prettier --write **/*.md",
    "clean": "rimraf .dist"
  },
  "bugs": {
    "url": "https://github.com/mikrokit/mikrokit/issues"
  },
  "dependencies": {
    "@deepkit/type": "^1.0.1-alpha.75",
    "@mikrokit/client": "^0.2.1-alpha.0",
    "@mikrokit/router": "^0.2.1-alpha.0",
    "i": "^0.3.7",
    "npm": "^9.4.0",
    "prettier": "^2.7.1",
    "ts-morph": "^17.0.1"
  },
  "devDependencies": {
    "@deepkit/core": "^1.0.1-alpha.75",
    "@deepkit/type-compiler": "^1.0.1-alpha.75",
    "@serverless/event-mocks": "^1.1.1"
  }
}
