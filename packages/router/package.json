{
  "name": "@mikrokit/router",
  "version": "0.2.0",
  "description": "RPC Like router with automatic Validation and Serialization",
  "keywords": [
    "typescript",
    "types",
    "models",
    "json",
    "schema",
    "generate"
  ],
  "author": "ma jerez <jerez.m82@gmail.com>",
  "homepage": "https://github.com/mikrokit/mikrokit/tree/main/packages/router#readme",
  "license": "MIT",
  "main": "./.dist/cjs/index.js",
  "module": "./.dist/esm/index.js",
  "types": "./.dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./.dist/cjs/index.js",
      "default": "./.dist/esm/index.js"
    }
  },
  "directories": {
    "lib": ".dist"
  },
  "files": [
    ".dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikrokit/mikrokit.git"
  },
  "scripts": {
    "test": "jest",
    "dev": "rimraf .dist && tsc --build tsconfig.build.json --watch",
    "dev:test": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "build:csj": "rimraf .dist/csj && tsc --build tsconfig.build.json",
    "build:esm": "rimraf .dist/esm && tsc --build tsconfig.build-esm.json",
    "build:": "npm run build:csj && npm run build:esm",
    "clean": "rimraf .dist"
  },
  "bugs": {
    "url": "https://github.com/mikrokit/mikrokit/issues"
  },
  "dependencies": {
    "@deepkit/type": "^1.0.1-alpha.75"
  },
  "devDependencies": {
    "@deepkit/core": "^1.0.1-alpha.75",
    "@deepkit/type-compiler": "^1.0.1-alpha.75"
  }
}
