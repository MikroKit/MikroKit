{
  "name": "@mikrokit/compiled-app",
  "version": "0.2.1-alpha.0",
  "private": true,
  "description": "Compiled app to be used in js libraries with needing to compile",
  "keywords": [
    "mikrokit compiled app"
  ],
  "author": "ma jerez <jerez.m82@gmail.com>",
  "homepage": "https://github.com/mikrokit/mikrokit/tree/main/packages/compiled-app#readme",
  "license": "MIT",
  "main": "./.dist/cjs/index.js",
  "module": "./.dist/esm/index.js",
  "types": "./.dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./.dist/cjs/index.js",
      "default": "./.dist/esm/index.js"
    }
  },
  "directories": {
    "lib": ".dist"
  },
  "files": [
    ".dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikrokit/mikrokit.git"
  },
  "scripts": {
    "test": "jest",
    "dev": "rimraf .dist && tsc --build tsconfig.build.json --watch",
    "dev:test": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "build:csj": "rimraf .dist/csj && tsc --build tsconfig.build.json",
    "build:esm": "rimraf .dist/esm && tsc --build tsconfig.build-esm.json",
    "build:": "npm run build:csj && npm run build:esm",
    "auto-readme": "embedme **/*.md && prettier --write **/*.md",
    "clean": "rimraf .dist"
  },
  "bugs": {
    "url": "https://github.com/mikrokit/mikrokit/issues"
  },
  "dependencies": {
    "@deepkit/framework": "^1.0.1-alpha.75",
    "@mikrokit/http": "^0.2.1-alpha.0",
    "ts-node": "^10.9.1"
  }
}
